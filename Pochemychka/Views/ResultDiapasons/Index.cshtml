@model IEnumerable<Pochemychka.Models.ResultDiapason>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IdTest)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StartDiapason)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EndDiapason)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TextResult)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdTest)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StartDiapason)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EndDiapason)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TextResult)
            </td>
            <td>
                    <a asp-action="Edit" asp-route-id="@item.IdDiapasonResult">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.IdDiapasonResult">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.IdDiapasonResult">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
